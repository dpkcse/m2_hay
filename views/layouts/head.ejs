                    var setCookie = (cname, cvalue, exdays) => {
                        var d = new Date();
                        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
                        var expires = "expires=" + d.toUTCString();
                        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
                    }

                    var getCookie = (cname) => {
                        var name = cname + "=";
                        var decodedCookie = decodeURIComponent(document.cookie);
                        var ca = decodedCookie.split(';');
                        for (var i = 0; i < ca.length; i++) {
                            var c = ca[i];
                            while (c.charAt(0) == ' ') {
                                c = c.substring(1);
                            }
                            if (c.indexOf(name) == 0) {
                                return c.substring(name.length, c.length);
                            }
                        }
                        return "";
                    }


                    function openProfilePage(element) {

                        var id = $(element).find('.box-header').attr('data-id');
                        var name = $(element).find('.box-header').attr('data-name');
                        var img = $(element).find('.box-header').attr('data-img');
                        var conversationid = $(element).find('.box-header').attr('data-conversationid');
                        var conversationtype = $(element).find('.box-header').attr('data-conversationtype');
                        var replyRootMsgID = $(element).find('.box-body').attr('data-msgid');
                        var replyRootMsgType = $(element).find('.box-body').attr('data-msgtype');
                        //alert(conversationid);

                        setCookie('userid', id, 1);
                        setCookie('username', name, 1);
                        setCookie('userimg', img, 1);
                        setCookie('conversationid', conversationid, 1);

                        if(replyRootMsgType == 'reply'){
                            setCookie('replyRootMsgID', replyRootMsgID, 1);
                        }else if(replyRootMsgType == 'direct'){
                            setCookie('replyRootMsgID', "" , 1);
                        }

                        if (conversationtype === 'personal') {
                            $.ajax({
                                type: 'POST',
                                data: {
                                    targetUser: name,
                                    targetID: id,
                                    ecosystem: 'Navigate'
                                },
                                dataType: 'json',
                                url: '/hayven/personalConCreate',
                                success: function(data) {
                                    window.location.href = '/hayven/chat/' + conversationtype + '/' + id + '/' + encodeURI(data.conversation_id) + '/' + encodeURI(name) + '/' + encodeURI(img);
                                },
                                error: function(err) {
                                    console.log(err);
                                }
                            });
                        } else if (conversationtype === 'group') {
                            window.location.href = '/hayven/chat/' + conversationtype + '/' + id + '/' + encodeURI(conversationid) + '/' + encodeURI(name) + '/' + encodeURI(img);
                        }


                    }

                    $(document).keyup(function(e) {
                        if (e.keyCode == 27) { // escape key maps to keycode `27`

                            if ($('#leftSideBar-flt').hasClass('moved')) {
                                $('#leftSideBar-flt').removeClass('moved');
                                $("#nav_tgl_img").show();
                                $("#nav_tgl_close").hide();
