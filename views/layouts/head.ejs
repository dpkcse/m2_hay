                    }


                    function isVisible(id) {
                        var element = $('#' + id);
                        if (element.length > 0 && element.css('visibility') !== 'hidden' && element.css('display') !== 'none') {
                            return true;
                        } else {
                            return false;
                        }
                    }


                    /** On mouse up event 'mouseup' */
                    $(document).mouseup(function(e) {
                        var container = "";
                        var leftSideBar = $('#leftSideBar-flt');
                        var targetID = "";
                        var tagContainer = "";
                        // console.log(e.target.id);
                        // console.log(197, e);


                        if ($('.notfDivHolder').is(':visible')) {
                            container = $('.notfDivHolder');
                            targetID = e.target.id;
                            tagContainer = "notfDivHolder";
                        } else if ($('.filter-btn-div').is(':visible')) {
                            container = $('.filter-btn-div');
                            targetID = e.target.id;
                            tagContainer = "filter-btn-div";
                        } else if ($('.action-btn-div').is(':visible')) {
                            if (!$(e.target).hasClass('pointer')) {
                                container = $('.action-btn-div');
                                targetID = e.target.id;
                                tagContainer = "action-btn-div";
                            }
                        } else {
                            targetID = "";
                            tagContainer = "";
                        }

                        if (e.target.id != 'nav_tgl_img' && container !== "" && targetID === "") {
                            if (!container.is(e.target) && container.has(e.target).length === 0) {
                                if(!$('#CreateReminder').is(':visible') && !$('#CreateEvent').is(':visible')){
                                container.hide();
                               }
                                if (tagContainer != "filter-btn-div" && tagContainer != "action-btn-div") {
                                    container.html("");
                                }

                                if (tagContainer == "action-btn-div" && $('#CreateReminder').is(':hidden') && $('#CreateEvent').is(':hidden')) {
                                    $('.backWrap').hide();
                                    $('.per-msg-action img').attr('src', '/images/NavbarIcons/actions-create_24px_FFF.png'); // This is per message action icon
                                    $('.action-btn').attr('src', '/images/NavbarIcons/actions-create_24px_FFF.png'); // This is message form action icon
                                    $('.message-container .per-msg').removeClass('active');
                                }

                                $("#tpgglequickDivOpen").hide();
                                $("#tpgglequickDiv").show();
                            }
                        }

                        if (e.target.id != 'nav-icon1' && leftSideBar !== "" && targetID === "") {
                            if (!leftSideBar.is(e.target) && leftSideBar.has(e.target).length === 0) {
                                if ($('#leftSideBar-flt').hasClass('moved')) {
                                    $("#nav_tgl_img").show();
                                    $("#nav_tgl_close").hide();
                                    $('#leftSideBar-flt').removeClass('moved');
                                }
                            }
                        }


                    });

