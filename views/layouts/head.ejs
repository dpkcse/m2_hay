                            if ($('#notfDivHolder').is(':visible')) {
                                $('#tpgglequickDiv').removeClass('open');
                                $("#notfDivHolder").html("");
                                $("#notfDivHolder").hide();
                                $("#tpgglequickDivOpen").hide();
                                $("#tpgglequickDiv").show();
                            }
                        }
                    });
                    // Toggle recent chat div at nav bar right side on click chat icon
                    var toggleRecentChat = function() {
                            $("#tpgglequickDivOpen").hide();
                            $("#tpgglequickDiv").show();
                            if ($("#notfDivHolder").is(':visible')) {

                                $("#notfDivHolder").html("");
                                $("#notfDivHolder").hide();

                            } else if ($("#notfDivHolder").is(':hidden')) {
                                $("#notfDivHolder").html($("#recentChatHoled").html());
